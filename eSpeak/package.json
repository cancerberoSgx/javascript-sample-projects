    "ts-refactor": "0.0.6",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "inferno": "^7.1.13",
    "inferno-create-element": "^7.1.13",
    "reflect-metadata": "^0.1.13"
  }
